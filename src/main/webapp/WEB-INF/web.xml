<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

	<servlet>
		<servlet-name>oauthUserInfo</servlet-name>
		<servlet-class>oauthUserInfo.Auth</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>oauthUserInfo</servlet-name>
		<url-pattern>/oauthUserInfoGoogle</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>oauthUserInfo</servlet-name>
		<url-pattern>/oauthUserInfoOrcid</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>oauthUserInfo</servlet-name>
		<url-pattern>/oauthUserInfoSynapse</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>oauthUserInfo</servlet-name>
		<url-pattern>/oauthUserInfoSynapseStaging</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>oauthUserInfo</servlet-name>
		<url-pattern>/oauthUserInfoSynapseForceAuth</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>oauthUserInfo</servlet-name>
		<url-pattern>/oauthUserInfoSynapseStagingForceAuth</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>oauthUserInfo</servlet-name>
		<url-pattern>/oauthUserInfoSynapseOmitCode</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>oauthUserInfo</servlet-name>
		<url-pattern>/oauthUserInfoSynapseStagingOmitCode</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>forbidden</servlet-name>
		<servlet-class>oauthUserInfo.Forbidden</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>forbidden</servlet-name>
		<url-pattern>/forbidden</url-pattern>
	</servlet-mapping>

</web-app>
